moveList: source to: target bottom: bottomBoolean 

		"call method to handle moving of items to different list"
    | select i |
    #CodeTrackerBlock.  "Last Modified  13:30:02 06/11/25"
    #LabWare. "Owner"
    #?. "Type"
    #?. "Project"
    #Button. "Module"
    #?. "Unit"
    #SS5. "RevisedBy"
    #NotReviewed. "ReviewedBy"
    #?. "LabTrackId"
    #CodeTrackerBlockEnd.  "Copyright © 2025 LabWare Inc."

    select := (self paneNamed: source) selectedItem.
    select ifNil: [^nil].
    i := (self paneNamed: source) selectedIndex.
    bottomBoolean
        ifTrue:[(self paneNamed: target) insertItem: select]
        ifFalse:[(self paneNamed: target) insertItem: select at: 1].
    (self paneNamed: source) deleteIndex: i.! !



