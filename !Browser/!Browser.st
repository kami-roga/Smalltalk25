	(Generated by WindowBuilder Pro)"
    Smalltalk unusedMemory.
    windows := OrderedCollection new.
    Browser allSubclasses do: [:aClass |
        aClass allInstancesPrim do: [:instance |
            instance views isEmpty
                ifFalse:[
                    instance views first isHandleOk
                        ifTrue:[
                            inst := BrowserListObject on: instance.
                            windows add: inst.]]]].
    ^(self paneNamed: 'browserList') contents: windows.! !



