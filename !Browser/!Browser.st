!BrowserListUI methods !
   
allColorCombos

	"reading and parsing each line of txt file into a rgb combo"
    | colorCombos fileStream lines |
    #CodeTrackerBlock.  "Last Modified  12:54:24 06/25/25"
    #LabWare. "Owner"
    #?. "Type"
    #?. "Project"
    #Browser. "Module"
    #?. "Unit"
    #SS5. "RevisedBy"
    #NotReviewed. "ReviewedBy"
    #?. "LabTrackId"
    #CodeTrackerBlockEnd.  "Copyright © 2025 LabWare Inc."

    "Open and read the file"
    fileStream := File pathNameReadOnly: 'savedColors.txt'.
    [lines := (fileStream contents) asString asArrayOfSubstringsSeparatedBy: Character lf.] ensure: [
        fileStream close.].
    colorCombos := OrderedCollection new.
    lines do: [:line |
        | label rgbParts fgString bgString fgRGB bgRGB fgColor bgColor|

	"Split name and rgb parts"
        label := (
            line asArrayOfSubstringsSeparatedBy: $:) first.
        rgbParts := (line asArrayOfSubstringsSeparatedBy: $:) at: 2.

	"Split fg and bg RGB strings"
        fgString := (rgbParts asArrayOfSubstringsSeparatedBy: $/) first.
        bgString := (rgbParts asArrayOfSubstringsSeparatedBy: $/) at: 2.

	"Convert strings to integer arrays"
        fgRGB := (fgString asArrayOfSubstringsSeparatedBy: $,) collect: [:s | s asInteger].
        bgRGB := (bgString asArrayOfSubstringsSeparatedBy: $,) collect: [:s | s asInteger].

	"Create Color instances"
        fgColor := Color red: (fgRGB at: 1) green: (fgRGB at: 2) blue: (fgRGB at: 3).
        bgColor := Color red: (bgRGB at: 1) green: (bgRGB at: 2) blue: (bgRGB at: 3).
        colorCombos add: (
            Array with: label with: fgColor with: bgColor).].
    ^colorCombos! !



!BrowserListUI methods !
  
closeSelected

		"removes object from list when selecting close"
    | selectedIndex selected currentContents |
    #CodeTrackerBlock.  "Last Modified  10:43:21 06/24/25"
    #LabWare. "Owner"
    #?. "Type"
    #?. "Project"
    #Browser. "Module"
    #?. "Unit"
    #SS5. "RevisedBy"
    #NotReviewed. "ReviewedBy"
    #?. "LabTrackId"
    #CodeTrackerBlockEnd.  "Copyright © 2025 LabWare Inc."

    selectedIndex := (self paneNamed: 'browserList') selection.
    selected := (self paneNamed: 'browserList') contents at: selectedIndex ifAbsent: [nil].
    selected ifNotNil: [
        selected close.
        currentContents := (self paneNamed: 'browserList') contents.
        (self paneNamed: 'browserList') contents: (currentContents reject: [:each | each == selected]).
        (self paneNamed: 'browserList') display.].! !



